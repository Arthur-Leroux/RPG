{
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "extends": "eslint:recommended",
    "rules": {
      "camelcase": 2,
      "semi": [
        "error",
        "always"
      ],
      "indent": ["error",2],
      "quotes": [
        "error",
        "backticks"
      ]
    },
    "env": {
      "browser": true
    }
  }